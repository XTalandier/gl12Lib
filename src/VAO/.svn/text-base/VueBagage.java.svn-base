package VAO;

/**
 *
 * @author Adri
 */
public class VueBagage extends DAO.DAO
{
    public String tb_name      = "ListeBagages";
    public String tb_primaire  = "NoBagage";
    public int    ch_NoBagage = 0;
    public float  ch_poids;
    public String ch_nom;
    public String ch_prenom;
    
    public VueBagage() {}
    public VueBagage(int id)
    {
        initWithID(id);
    }

    public int getCh_NoBagage() {
        return ch_NoBagage;
    }

    public void setCh_NoBagage(int ch_NoBagage) {
        this.ch_NoBagage = ch_NoBagage;
    }

    public String getCh_nom() {
        return ch_nom;
    }

    public void setCh_nom_client(String ch_nom) {
        this.ch_nom = ch_nom;
    }

    public float getCh_poids() {
        return ch_poids;
    }

    public void setCh_poids(float ch_poids) {
        this.ch_poids = ch_poids;
    }

    public String getCh_prenom() {
        return ch_prenom;
    }

    public void setCh_prenom_client(String ch_prenom) {
        this.ch_prenom = ch_prenom;
    }
    
    

      
} // VueBagage
