package VAO;

import DAO.DAO;

/**
 *
 * @author Xavier TALANDIER
 */
public class Client extends DAO {
	public String tb_name      = "client";
	public String tb_primaire  = "id_client";
	public int    ch_id_client = 0;
	public String ch_nom;
	public String ch_prenom;
	public String ch_email;

	public Client() {
	}

	public Client(int id) {
		initWithID(id);
	}

	/**
	 * @return the ch_id_client
	 */
	public int getCh_id_client() {
		return ch_id_client;
	}

	/**
	 * @param ch_id_client the ch_id_client to set
	 */
	public void setCh_id_client(int ch_id_client) {
		this.ch_id_client = ch_id_client;
	}

	/**
	 * @return the ch_nom
	 */
	public String getCh_nom() {
		return ch_nom;
	}

	/**
	 * @param ch_nom the ch_nom to set
	 */
	public void setCh_nom(String ch_nom) {
		this.ch_nom = ch_nom;
	}

	/**
	 * @return the ch_prenom
	 */
	public String getCh_prenom() {
		return ch_prenom;
	}

	/**
	 * @param ch_prenom the ch_prenom to set
	 */
	public void setCh_prenom(String ch_prenom) {
		this.ch_prenom = ch_prenom;
	}

	/**
	 * @return the ch_email
	 */
	public String getCh_email() {
		return ch_email;
	}

	/**
	 * @param ch_email the ch_email to set
	 */
	public void setCh_email(String ch_email) {
		this.ch_email = ch_email;
	}

}